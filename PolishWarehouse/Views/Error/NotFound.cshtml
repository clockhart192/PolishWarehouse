@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Page Not Found";
}

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3>Page Nout Found</h3>
            </div>
            <div class="panel-collapse" id="Polishcollapse">
                <div class="panel-body text-center">
                    <div class="row">
                        <div class="col-lg-12">
                            <h2>We looked hard and could not find the page you wanted.</h2>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <img src="~/Content/Images/404.jpg" class="error-image" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
